<h1>Database of StudentInfo</h1>
<table border="1">
  <tr>
    <th>student_id</th><th>student_name</th><th>score_toeic</th><th>score_toefl</th><th>score_eiken</th><th>theme</th><th>created_at</th>
  </tr>
  <% @data.each do |obj| %>
    <tr>
      <td><%= obj.student_id %></td>
      <td><%= obj.student_name %></td>
      <td><%= obj.score_toeic %></td>
      <td><%= obj.score_toefl %></td>
      <td><%= obj.score_eiken %></td>
      <td><%= obj.theme %></td>
      <td><%= obj.created_at %></td>      
    </tr>
  <% end %>
</table>
